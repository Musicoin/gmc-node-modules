    "clean": "rm -rf {src,bin/gmc,*tmp}",
    "clone": "echo 'getting source...\n' & git clone https://github.com/Musicoin/go-musicoin.git src && echo 'âˆš'",
    "finish": "rm -f ./bin/gmc && mv src/build/bin/gmc ./bin/gmc",
    "test": "./bin/gmc version"
  }
}
