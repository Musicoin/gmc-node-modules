{
  "name": "musicoin-daemon",
  "version": "1.5.1",
  "description": "a go-musicoin binary for node",
  "license": "MIT",
  "private": false,
  "homepage": "https://musicoin.org",
  "bugs": {
    "mail": "13@etaletai.com",
    "web": "https://github.com/etaletai13/gmc/issues"
  },
  "issues": "https://github.com/etaletai13/gmc/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/etaletai13/gmc.git"
  },
  "bin": {
    "gmc-linux-386": "./go-musicoin/build/bin/gmc-linux-386",
    "gmc-linux-amd64": "./go-musicoin/build/bin/gmc-linux-amd64",
    "gmc-windows-386": "./go-musicoin/build/bin/gmc-windows-4.0-386.exe",
    "gmc-windows-amd64": "./go-musicoin/build/bin/gmc-windows-4.0-amd64"
  },
  "engines": {
    "yarn": ">=1.7.0"
  },
  "keywords": [
    "go-musicoin",
    "musicoin",
    "blockchain",
    "gmc",
    "wallet"
  ],
  "scripts": {
    "preinstall": "if ! [ -d go-musicoin ]; then git clone https://github.com/Musicoin/go-musicoin.git; else yarn clean; fi",
    "clean": "rm -fr go-musicoin/build/_workspace/pkg/ go-musicoin/bin",
    "patch": "sed -i.tmp 's/1.7/1.10/g' ./go-musicoin/build/ci.go",
    "install": "sh ./index.sh",
    "test": "ls go-musicoin/build/bin"
  }
}
