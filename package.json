{
  "name": "@etaletai13/gmc-bin",
  "version": "1.0.0-beta",
  "description": "makes available go-musicoin binaries in node projects",
  "license": "MIT",
  "private": false,
  "homepage": "https://musicoin.org",
  "bugs": {
    "mail": "13@etaletai.com",
    "web": "https://github.com/Musicoin/desktop/issues"
  },
  "issues": "https://github.com/Musicoin/desktop/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/Musicoin/gmc-bin.git"
  },
  "bin": {
    "gmc": "./gmc-*"
  },
  "keywords": [
    "blockchain",
    "crypto",
    "gmc",
    "gmc-bin",
    "go-musicoin",
    "music",
    "musicoin",
    "wallet"
  ],
  "scripts": {
    "prepare": "yarn clean && yarn clone && yarn patch && yarn build && yarn permissions && yarn placement",
    "clean": "rm -rf ./gmc ./go-musicoin",
    "clone": "git clone git@github.com:Musicoin/go-musicoin.git",
    "patch": "sed -i 's/1.7/1.10/g' go-musicoin/build/ci.go",
    "build": "yarn build.darwin && yarn build.linux && yarn build.windows",
    "build.darwin": "cd go-musicoin && make gmc-darwin",
    "build.windows": "cd go-musicoin && make gmc-windows",
    "build.linux": "cd go-musicoin && make gmc-linux-amd64",
    "permissions": "chmod +x go-musicoin/build/bin/gmc",
    "placement": "mv go-musicoin/build/bin/gmc .",
    "test": "ls gmc && rm -rf go-musicoin"
  }
}
